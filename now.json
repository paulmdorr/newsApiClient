{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "src/api/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api/news/(.*)?", "dest": "/src/api/index.js?category=$1" },
    { "src": "/service-worker.js", "dest": "/static/service-worker.js" }
  ],
  "build": {
    "env": {
      "NEWS_API_KEY": "@newsapikey"
    }
  }
}
